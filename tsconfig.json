{
	"extends": ["@sapphire/ts-config", "@sapphire/ts-config/extra-strict", "@sapphire/ts-config/decorators"],
	"compilerOptions": {
		"outDir": "dist",
		"skipLibCheck": true,
		"removeComments": true,
		"noImplicitAny": false,
		"strictNullChecks": false,
		"useUnknownInCatchVariables": false
	},
	"include": ["src"]
}
