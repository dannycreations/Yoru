		"@vegapunk/request": "^1.0.11",
		"@vegapunk/utilities": "^1.0.17",
		"clashofclans.js": "^3.3.12",
		"discord.js": "14.x",
		"dotenv": "^16.4.5",
		"lokijs": "^1.5.12"
	},
	"devDependencies": {
		"@sapphire/ts-config": "^5.0.1",
		"@types/lokijs": "^1.5.14",
		"@types/node": "20.x",
		"tsc-watch": "^6.2.0",
		"typescript": "^5.6.2"
	}
